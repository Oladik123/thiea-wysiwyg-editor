{
  "name": "wysiwyg",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "-": "^0.0.1",
    "@theia/core": "latest",
    "dev": "^0.1.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "react-beautiful-dnd": "^13.0.0",
    "react-draggable": "^4.2.0"
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "build:css": "node-sass src/browser/style/sass -o src/browser/style/css",
    "watch:css": "yarn run build:css && node-sass src/browser/style/sass -o src/browser/style/css --watch --recursive "
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/wysiwyg-frontend-module"
    }
  ]
}
